  /**
   * Get a specific prompt
   */
  async getPrompt(name, args) {
    /* istanbul ignore next */
    cov_1zfd3argxf().f[32]++;
    cov_1zfd3argxf().s[179]++;
    switch (name) {
      case 'drupal_content_summary':
        /* istanbul ignore next */
        cov_1zfd3argxf().b[48][0]++;
        cov_1zfd3argxf().s[180]++;
        return this.getDrupalContentSummaryPrompt(args);
      default:
        /* istanbul ignore next */
        cov_1zfd3argxf().b[48][1]++;
        cov_1zfd3argxf().s[181]++;
        throw new Error(`Unknown prompt: ${name}`);
    }
  }
  /**
   * Get Drupal content summary prompt
   */
  async getDrupalContentSummaryPrompt(args) {
    /* istanbul ignore next */
    cov_1zfd3argxf().f[33]++;
    const contentType =
    /* istanbul ignore next */
    (cov_1zfd3argxf().s[182]++, args?.content_type);
    const limit =
    /* istanbul ignore next */
    (cov_1zfd3argxf().s[183]++,
    /* istanbul ignore next */
    (cov_1zfd3argxf().b[49][0]++, args?.limit) ||
    /* istanbul ignore next */
    (cov_1zfd3argxf().b[49][1]++, 10));
    /* istanbul ignore next */
    cov_1zfd3argxf().s[184]++;
    try {
      const nodes =
      /* istanbul ignore next */
      (cov_1zfd3argxf().s[185]++, await this.drupalClient.getNodes({
        type: contentType,
        limit
      }));
      const content =
      /* istanbul ignore next */
      (cov_1zfd3argxf().s[186]++, `Here are the recent Drupal nodes${contentType ?
      /* istanbul ignore next */
      (cov_1zfd3argxf().b[50][0]++, ` of type "${contentType}"`) :
      /* istanbul ignore next */
      (cov_1zfd3argxf().b[50][1]++, '')}:\n\n${nodes.map((node, index) => {
        /* istanbul ignore next */
        cov_1zfd3argxf().f[34]++;
        cov_1zfd3argxf().s[187]++;
        return `${index + 1}. ${node.attributes.title} (ID: ${node.attributes.nid})`;
      }).join('\n')}\n\nPlease provide a summary of this content.`);
      /* istanbul ignore next */
      cov_1zfd3argxf().s[188]++;
      return {
        description: `Summary of Drupal content${contentType ?
        /* istanbul ignore next */
        (cov_1zfd3argxf().b[51][0]++, ` (${contentType})`) :
        /* istanbul ignore next */
        (cov_1zfd3argxf().b[51][1]++, '')}`,
        messages: [{
          role: 'user',
          content: {
            type: 'text',
            text: content
          }
        }]
      };
    } catch (error) {
      const errorMessage =
      /* istanbul ignore next */
      (cov_1zfd3argxf().s[189]++, error instanceof drupal_client_js_1.DrupalClientError ?
      /* istanbul ignore next */
      (cov_1zfd3argxf().b[52][0]++, error.message) :
      /* istanbul ignore next */
      (cov_1zfd3argxf().b[52][1]++, String(error)));
      /* istanbul ignore next */
      cov_1zfd3argxf().s[190]++;
      return {
        description: 'Error generating Drupal content summary',
        messages: [{
          role: 'user',
          content: {
            type: 'text',
            text: `Error fetching Drupal content: ${errorMessage}`
          }
        }]
      };
    }
  }
