  /**
   * Extract description from tutorial content for RAG optimization
   */
  extractDescriptionFromContent(content) {
    /* istanbul ignore next */
    cov_1zfd3argxf().f[28]++;
    // Extract first paragraph or first 200 characters as description
    const lines =
    /* istanbul ignore next */
    (cov_1zfd3argxf().s[171]++, content.split('\n').filter(line => {
      /* istanbul ignore next */
      cov_1zfd3argxf().f[29]++;
      cov_1zfd3argxf().s[172]++;
      return line.trim();
    }));
    const firstContentLine =
    /* istanbul ignore next */
    (cov_1zfd3argxf().s[173]++, lines.find(line => {
      /* istanbul ignore next */
      cov_1zfd3argxf().f[30]++;
      cov_1zfd3argxf().s[174]++;
      return /* istanbul ignore next */(cov_1zfd3argxf().b[45][0]++, !line.startsWith('#')) &&
      /* istanbul ignore next */
      (cov_1zfd3argxf().b[45][1]++, line.trim().length > 0);
    }));
    /* istanbul ignore next */
    cov_1zfd3argxf().s[175]++;
    if (firstContentLine) {
      /* istanbul ignore next */
      cov_1zfd3argxf().b[46][0]++;
      cov_1zfd3argxf().s[176]++;
      return firstContentLine.length > 200 ?
      /* istanbul ignore next */
      (cov_1zfd3argxf().b[47][0]++, `${firstContentLine.substring(0, 197)}...`) :
      /* istanbul ignore next */
      (cov_1zfd3argxf().b[47][1]++, firstContentLine);
    } else
    /* istanbul ignore next */
    {
      cov_1zfd3argxf().b[46][1]++;
    }
    // Fallback: return first 200 characters
    cov_1zfd3argxf().s[177]++;
    return `${content.substring(0, 197)}...`;
  }
